<section id="{{ params.primary }}s">
   	<div class="overlay"></div>
   	<div class="gradient-overlay"></div>   	
   	<div class="row narrow section-intro with-bottom-sep animate-this">
   		<div class="col-full">   			
        <h3>{{ params.primary }}s</h3>
        <ul class="{{ params.primary }}">
        {% for item in params.data %}
          {# <li class="{{ params.secondary }}"><a href="{{item.url}}" target="_blank"><i class="{{item.icon}}"></i>{{item.title}}</a></li> #}
          <li class="{{ params.secondary }}"><a href="{{item.url}}" target="_blank">{% icon item.icon}}{{item.title}}</a></li>
        {% endfor %}
        </ul>
      </div>
    </div>
</section>