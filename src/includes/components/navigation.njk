<a id="header-menu-trigger" href="#0">
    <span class="header-menu-text">Menu</span>
    <span class="header-menu-icon"></span>
</a>
<nav id="menu-nav-wrap">
    <a href="#0" class="close-button" title="close"><span>Close</span></a>	
    <h3><a href="#top">TulsaTechFest</a></h3>  
    <ul class="nav-list">
        {%- for item in collections.all | eleventyNavigation -%}
        {%- if item.url -%}
            {%- set isCurrent = (item.url == page.url) -%}
            {%- set isParent = item.url != '/' and item.url in page.url -%}
            <li class="{% if isCurrent or isParent %}current{% endif %}">
                <a
                    class="smoothscroll"
                    href="{% if isCurrent %}#main{% else %}{{ item.url | url }}{% endif %}"
                    {% if isCurrent %}aria-current="page"{% endif %}
                    title="{{ item.title }}"
                >
                    {{ item.title }}
                </a>
            </li>
        {%- endif -%}
    {%- endfor -%}					
        {# <li><a href="/admin/" {% if isCurrent %}aria-current="page"{% endif %} title="Admin">Admin</a></li> #}
    </ul>	
    <ul class="nav-list2">
    {% for item in collections.alerts %}
        {# <p class="alert-text"><a href="{{item.url}}">{{item.data.text}}</a></p> #}
        <li><a href="{{item.data.url}}">{{item.data.text}}</a></li>
    {% endfor %}
    </ul>
    <ul class="header-social-list">
        {% for item in collections.links %}
            <li><a href="{{item.data.url}}" target="_blank"><i class="{{item.data.icon}}"></i></a></li>
        {% endfor %}
    </ul>
    {# {{ component('socialLinks', {
        primary: 'header-social-list',
        secondary: 'list-item',
        data: collections.links
    }) }}
    {% list "header-social-list", "list-item", "icon,link", collections.links %} #}
        
</nav>
